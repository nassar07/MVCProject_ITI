@model List<ApplicationUser>
@{
    ViewData["Title"] = "UsersList";
}

<h1>UsersList</h1>


<table class="table table-bordered table-hover">
	<thead>
		<tr>
			<th>Id</th>
			<th>Username</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in Model )
		{
			<tr>
				<td>@user.Id</td>
				<td>@user.UserName</td>
				<td>@user.Email</td>
			</tr>
		}
	</tbody>

